FLAGGED? :XYZZY
COPY %Tr %A
PRINT "xyzzy is initially %i\n"

FLAG 1 :XYZZY
FLAGGED? :XYZZY
COPY %Tr %A
PRINT "xyzzy is %i after FLAG 1 call\n"

FLAG 0 :XYZZY
FLAGGED? :XYZZY
COPY %Tr %A
PRINT "xyzzy is %i after FLAG 0 call\n"

FLAG 425 :XYZZY
FLAGGED? :XYZZY
COPY %Tr %A
PRINT "xyzzy is %i after FLAG 425 call\n"
